pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                checkout scm
                bat 'docker build -t dotnet .'
            }  
        }
        stage('UnitTest') {
            steps {
                script {
                bat label: 'Unit Test using Dotnet CLI',
                script: '''
                dotnet.exe test .\\MVC Test Project\\
                '''
        }
      }
    }
    
    }
}